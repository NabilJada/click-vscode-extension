{
  "Elementclass Template": {
    "prefix": "elementclass",
    "body": [
      "elementclass ${1:Name} {",
      "\tinput -> ${2:x} :: ${3:Element} -> output;",
      "\tinput [1] -> ${4:y} :: ${5:Element} -> [1] output;",
      "};"
    ],
    "description": "Click elementclass template"
  },
  "Simple Connection": {
    "prefix": "conn",
    "body": [
      "${1:src} :: ${2:Element1} -> ${3:dst} :: ${4:Element2};"
    ],
    "description": "Basic element connection"
  },
  "Two-Port Element": {
    "prefix": "twocon",
    "body": [
      "${1:src} :: ${2:Element1} -> ${3:x} :: ${4:Element2} -> ${5:dst} :: ${6:Element3};",
      "${1:src} [1] -> ${7:y} :: ${8:Element4} -> [1] ${5:dst};"
    ],
    "description": "Two-port element wiring"
  }
}